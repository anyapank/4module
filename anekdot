function generate() {
    var jokes = [
        '<img src="joke1.png">',
        '<img src="joke2.png">',
        '<img src="joke3.png">',
        '<img src="joke4.png">',
        ]

        var lol=document.querySelector(".div-img");
        lol.innerHTML=jokes[Math.floor(Math.random()*jokes.length)];

}